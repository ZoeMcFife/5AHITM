        "sns.pairplot(df,\n",
        "             vars=['Pedestrians', 'temperature', 'precipitation', 'hour', 'month'],\n",
        "             diag_kind = 'kde',\n",
        "             plot_kws={'alpha':0.5, 'size': 0.1})"
      ],
